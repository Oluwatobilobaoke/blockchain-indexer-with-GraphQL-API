{
  "name": "blockchain-indexer",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon src/app.ts",
    "build": "sh build.sh",
    "start": "node dist/app.js",
    "dev1": "npx postgraphile -c supabase -s app --watch -p 4000",
    "dev2": "npx postgraphile -c supabase -s app --watch -p 4000 --enhance-graphiql",
    "dev3": "npx postgraphile -c supabase -s app --watch -p 4000 --append-plugins @graphile-contrib/pg-simplify-inflector",
    "dev4": "npx postgraphile -c supabase -s app --watch -p 4000 --append-plugins @graphile-contrib/pg-simplify-inflector --enhance-graphiql"
  },
